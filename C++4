//Compiler version g++ 6.3.0

int main()
{
    int a,b,c,d;
    int aksi;
    
    cout <<"kehadiran :";
    cin >> a;
    cout <<"tugas :";
    cin >> b;
    cout <<"UTS :";
    cin >> c;
    cout <<"UAS :";
    cin >> d;
    
    aksi = (a+b+c+d)/4;
    cout <<"nilai akhir" <<aksi <<endl;
    
    
    
